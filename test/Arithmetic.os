proc main: void()
{
  i: int = 13;
  j: int = 8;
  print(i, " <op> ", j, '\n');
  print("+ ", i + j, '\n');
  print("- ", i - j, '\n');
  print("* ", i * j, '\n');
  print("/ ", i / j, '\n');
  print("% ", i % j, '\n');
  print("^ ", i ^ j, '\n');
  print("& ", i & j, '\n');
  print("| ", i | j, '\n');
  print("<< ", i << j, '\n');
  print(">> ", i >> j, '\n');
}
